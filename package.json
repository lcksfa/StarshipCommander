{
  "name": "starship-commander-habits",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Starship Commander Habits - AI-powered task management with NestJS + tRPC + React",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:backend": "nest start --watch --preserveWatchOutput",
    "build:backend": "cd src/backend && nest build",
    "start:backend": "node src/backend/dist/main",
    "start:debug": "nest start --debug --watch",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run dev:backend\"",
    "build:all": "pnpm run build && pnpm run build:backend",
    "start:prod": "pnpm run build:all && pnpm run start:backend",
    "install:all": "pnpm install",
    "clean": "rimraf dist node_modules/.cache pnpm-lock.yaml",
    "clean:full": "pnpm run clean && rimraf node_modules",
    "reinstall": "pnpm run clean:full && pnpm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:backend": "jest --config jest.backend.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint \"{src/frontend,src/backend}/**/*.{ts,tsx}\" --fix",
    "lint:frontend": "eslint \"src/frontend/**/*.{ts,tsx}\" --fix",
    "lint:backend": "eslint \"src/backend/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "nest": "nest",
    "typecheck": "tsc --noEmit",
    "typecheck:backend": "cd src/backend && tsc --noEmit",
    "pnpm:list": "pnpm list --depth=0",
    "pnpm:outdated": "pnpm outdated",
    "pnpm:update": "pnpm update --latest",
    "pnpm:audit": "pnpm audit",
    "pnpm:audit:fix": "pnpm audit --fix",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "ports:clean:all": "./scripts/cleanup-ports.sh --all",
    "ports:clean:frontend": "./scripts/cleanup-ports.sh 3000",
    "ports:clean:backend": "./scripts/cleanup-ports.sh 3001",
    "ports:clean:dev": "./scripts/cleanup-ports.sh 3000 3001 3002 3003 3004 3005"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/client": "^6.2.0",
    "@trpc/server": "^11.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "helmet": "^8.1.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.23.0",
    "@eslint/object-schema": "^3.0.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@trpc/client": "^11.8.1",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "lru-cache": "^11.2.4",
    "prettier": "^3.7.4",
    "prisma": "^6.2.0",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": ".",
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.spec.{ts,tsx}",
      "!src/**/*.test.{ts,tsx}"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.26.1",
  "keywords": [
    "react",
    "nestjs",
    "trpc",
    "typescript",
    "monorepo",
    "ai",
    "starship",
    "habits",
    "task-management"
  ],
  "author": "Your Name",
  "license": "MIT"
}
